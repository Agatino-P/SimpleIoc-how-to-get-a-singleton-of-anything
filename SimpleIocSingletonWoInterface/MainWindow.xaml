<Window x:Class="SimpleIocSingletonWoInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SimpleIocSingletonWoInterface"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow" Width="800" Height="450" mc:Ignorable="d">
    <UniformGrid>
        <Button x:Name="tstButton1" Width="200" Height="100 "
                HorizontalAlignment="Center" VerticalAlignment="Center"
                Click="tstButton1_Click" Content="singleton" />
        <Button x:Name="tstButton2" Width="200" Height="100 "
                HorizontalAlignment="Center" VerticalAlignment="Center"
                Click="tstButton2_Click" Content="Parametric Constructor" />
        <Button x:Name="tstButton3" Width="200" Height="100 "
                HorizontalAlignment="Center" VerticalAlignment="Center"
                Click="tstButton3_Click" Content="Generic Interface 1" />

    </UniformGrid>
</Window>
